! URXVT GENERAL CONFIG
!------------------------------------------------
URxvt.geometry: 90x16
URxvt.imLocale: en_US.UTF-8

! URXVT FONTS
!------------------------------------------------
URxvt.font:xft:monaco:size=10,xft:Source\ Code\ Pro\ for\ Powerline:size=10
URxvt.boldFont:xft:monaco:bold:size=10,xft:Source\ Code\ Pro\ for\ Powerline:size=10
URxvt.italicfont:xft:monaco:italic:size=10,xft:Source\ Code\ Pro\ for\ Powerline:size=10
URxvt.bolditalicFont:xft:monaco:bold:italic:size=10,xft:Source\ Code\ Pro\ for\ Powerline:size=10
URxvt*letterSpace: -1
URxvt*allow_bold: true
! URXVT FONT SETTINGS
!------------------------------------------------
Xft.dpi:        97
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
URxvt.shading:       20



URxvt*perl-ext:
URxvt*perl-ext-common:



!------------------------------------------------
URxvt.iso14755: False
! URXVT SCROLLBAR AND CURSOR STYLE
!------------------------------------------------
URxvt.saveLines: 3000
URxvt.scrollBar: false
URxvt.scrollstyle: rxvt
URxvt.cursorBlink: true
URxvt.cursorUnderline: true
!==================================================
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007


